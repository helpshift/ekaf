[project]
	description = A minimal, high-performance Kafka client in Erlang.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Bhasker Kode (bosky101)
[access "refs/for/refs/*"]
	read = group user/Bhasker Kode (bosky101)
	push = group user/Bhasker Kode (bosky101)
	label-Code-Review = -2..+2 group user/Bhasker Kode (bosky101)
	label-Verified = -1..+1 group user/Bhasker Kode (bosky101)
	rebase = group user/Bhasker Kode (bosky101)
	removeReviewer = group user/Bhasker Kode (bosky101)
	submit = group user/Bhasker Kode (bosky101)
[access "refs/heads/*"]
	read = group user/Bhasker Kode (bosky101)
	create = group user/Bhasker Kode (bosky101)
	push = +force group user/Bhasker Kode (bosky101)
	pushMerge = group user/Bhasker Kode (bosky101)
[access "refs/tags/*"]
	push = +force group user/Bhasker Kode (bosky101)
	pushTag = group user/Bhasker Kode (bosky101)
	pushSignedTag = group user/Bhasker Kode (bosky101)
